{
  "name": "claude-frontend",
  "version": "0.0.1",
  "description": "Visual element inspector for Claude Code - select elements in your browser and send them to Claude for instant code modifications",
  "main": "index.js",
  "browser": "browser.js",
  "exports": {
    ".": {
      "browser": "./browser.js",
      "node": "./index.js",
      "import": "./browser.js",
      "require": "./index.js",
      "default": "./browser.js"
    },
    "./auto": {
      "import": "./auto.js",
      "require": "./auto.js",
      "default": "./auto.js"
    },
    "./widget": {
      "import": "./lib/widget.js",
      "require": "./lib/widget.js",
      "default": "./lib/widget.js"
    },
    "./server": {
      "import": "./lib/server.js",
      "require": "./lib/server.js",
      "default": "./lib/server.js"
    }
  },
  "bin": {
    "claude-frontend": "./bin/cli.js"
  },
  "scripts": {
    "start": "node bin/cli.js",
    "dev": "node bin/cli.js",
    "claude": "node bin/cli.js",
    "test": "node -e \"console.log('âœ… Package structure valid')\"",
    "prepare": "echo 'Ready for installation'",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "claude-code",
    "claude",
    "ai",
    "frontend",
    "development",
    "devtools",
    "debugging",
    "visual-selection",
    "element-inspector",
    "react",
    "nextjs",
    "vue",
    "svelte"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tobias-schuemann/claude-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/tobias-schuemann/claude-frontend/issues"
  },
  "homepage": "https://github.com/tobias-schuemann/claude-frontend#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "bin/",
    "lib/",
    "index.js",
    "browser.js",
    "index.mjs",
    "index.d.ts",
    "auto.js",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "types": "index.d.ts",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "semantic-release": "^22.0.12"
  }
}
